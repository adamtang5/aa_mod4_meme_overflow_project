include utils.pug
#project-info
    a#project-name(href="https://github.com/adamtang5/aa_mod4_meme_overflow_project")
        .footer-icons
            +githubIcon("#000000")
        span MemeOverflow Project
    span Created with
    ul#feature-technologies
        li: a(href="https://developer.mozilla.org/en-US/docs/Web/JavaScript")
            .footer-icons
                +jsIcon("#000000")
        li: a(href="https://expressjs.com")
                img.footer-icons(src="/icons/express-js-node-js.png", alt="Express.js")
        li: a(href="https://postgresql.org")
            img.footer-icons(src="https://postgresql.org/media/img/about/press/elephant.png", alt="Postgresql")
        li: a(href="https://pugjs.org")
            img.footer-icons(src="/icons/pugjs.png", alt="Pug JS")
        li: a(href="https://sequelize.org/v5/manual/")
            img.footer-icons(src="https://sequelize.org/v5/manual/asset/logo-small.png", alt="Sequelize")
        li: a(href="https://developer.mozilla.org/en-US/docs/Glossary/HTML5")
            .footer-icons
                +html5Icon("#000000")
        li: a(href="https://developer.mozilla.org/en-US/docs/Web/CSS")
            .footer-icons
                +css3Icon("#000000")
